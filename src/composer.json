{
    "name": "egamings/telegram-bot-sg-sales",
    "description": "Telegram bot for sales",
    "type": "project",
    "require": {
        "php": "^8.1",
        "illuminate/config": "^12.26",
        "illuminate/container": "^12.24",
        "illuminate/database": "^12.24",
        "illuminate/events": "^12.24",
        "illuminate/filesystem": "^12.26",
        "illuminate/redis": "^12.24",
        "illuminate/support": "^12.24",
        "illuminate/translation": "^12.26",
        "irazasyed/telegram-bot-sdk": "^3.0",
        "mdma4d/laravel-vault": "^1.1",
        "predis/predis": "^3.2",
        "vlucas/phpdotenv": "^5.6"
    },
    "require-dev": {
        "phpunit/phpunit": "^10.0"
    },
    "autoload": {
        "psr-4": {
            "App\\": "app/classes/",
            "Database\\Factories\\": "database/factories/",
            "Database\\Seeders\\": "database/seeders/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Tests\\": "tests/"
        }
    },
    "scripts": {
        "post-autoload-dump": [
            
        ],
        "post-update-cmd": [
            
        ],
        "post-root-package-install": [
            "@php -r \"file_exists('.env') || copy('.env.example', '.env');\""
        ],
        "post-create-project-cmd": [
            
        ]
    },
    "config": {
        "allow-plugins": {
            "php-http/discovery": true,
            "pestphp/pest-plugin": true
        },
        "optimize-autoloader": true,
        "preferred-install": "dist",
        "sort-packages": true
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}
